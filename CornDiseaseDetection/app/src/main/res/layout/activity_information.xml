        app:layout_constraintStart_toEndOf="@+id/imageView_maiz"
        app:layout_constraintTop_toBottomOf="@+id/txt_nomb_ingles" />

    <TextView
        android:id="@+id/txt_otros_nombres"
        android:layout_width="182dp"
