        android:text="Informacion obtenida de:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="575dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="369dp"
        android:layout_height="19dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Consultar mas enfermedades:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="630dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_informacion_obt"
        android:layout_width="369dp"
        android:layout_height="19dp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="602dp"
        android:textColor="#2232D1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_cons_enfermedades"
        android:layout_width="369dp"
        android:layout_height="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="656dp"
        android:textColor="#2232D1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_descripcion"
        android:layout_width="367dp"
        android:layout_height="244dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="300dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>